import javax.lang.model.util.ElementScanner6;

class Test
{
public static void main(String args[])
{
int i,j;
for(i=1;i<=5;i++)
{
    for(j=1;j<=6;j++)
    {
        if(i==1 || i==5 || j==1 || j==6)
        System.out.print('*');
        else
        System.out.print('.');
    }
    System.out.println();
}
}
}


